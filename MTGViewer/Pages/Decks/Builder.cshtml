@page "{id:int?}"
@model Decks.BuilderModel
@{
    string title = Model.DeckName is null
        ? "New Deck"
        : $"{Model.DeckName} - Builder";

    ViewData["Title"] = title;
}

<component type="typeof(DeckBuilder)" render-mode="ServerPrerendered" 
    param-UserId="Model.UserId"
    param-DeckId="Model.DeckId"
    param-PageSize="Model.PageSize" />